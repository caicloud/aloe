{
  "Vendor": true,
  "DisableAll": true,
  "Enable": [
    "vet",
    "safesql",
    "errcheck",
    "goconst",
    "goimports",
    "varcheck",
    "gas",
    "megacheck",
    "lll",
    "unconvert",
    "misspell",
    "golint",
    "deadcode",
